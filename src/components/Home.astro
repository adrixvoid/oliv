---
import Container from "./ui/container/Container.astro";
import Button from "./ui/button/Button.astro";
import Carousel from "./ui/carousel/Carousel.astro";
import ServiceCard from "./ui/card/ServiceCard.astro";
import ValuesCard from "./ui/card/ValuesCard.astro";
import PhotoCard from "./ui/card/PhotoCard.astro";
import Flex from "./ui/flex/Flex.astro";
import Heading from "./ui/text/Heading.astro";
import Text from "./ui/text/Text.astro";
import Card from "./ui/card/Card.astro";
import ProfileCard from "./ui/card/ProfileCard.astro";

import MainHeader from "./MainHeader.astro";
import MainFooter from "./MainFooter.astro";
import styles from "./Home.module.css";

import work from "../data/work.json";
import blog from "../data/blog.json";

import BrandingCard from "./ui/card/BrandingCard.astro";
---

<MainHeader />
<main>
  <section id="hero">
    <Container class={styles.hero}>
      <div>
        <div class={styles.heroWrapper}>
          <Heading element="h1" size="h1" style="line-height: 1.1;">
            Building Brands, One Success Story at a Time.
          </Heading>
          <Text size="xl" balanced="true" style="width: 40ch">
            Create a Brand That Resonates: We Can Help You Define and
            Communicate Your Unique Message.
          </Text>
          <Button variant="primary" style="margin-top:2rem">
            Our Services
          </Button>
        </div>
      </div>
    </Container>
  </section>
  <section id="logos">
    <Container>
      <Carousel />
    </Container>
  </section>
  <section id="services">
    <Container class="spacing-top">
      <div class="grid-4-desktop grid-2-tablet">
        <ServiceCard>
          <Fragment slot="title"> Brand Strategy </Fragment>
          <Fragment slot="description">
            <p>
              We help businesses define their unique brand message and branding
              positioning.
            </p>
          </Fragment>
          <Fragment slot="footer">
            <div>
              <a href="/services/#strategy">About Strategy</a>
            </div>
          </Fragment>
        </ServiceCard>
        <ServiceCard>
          <Fragment slot="title"> Visual Identity </Fragment>
          <Fragment slot="description">
            <p>
              We create visual elements such as logos, packaging, and marketing
              materials for your brand.
            </p>
          </Fragment>
          <Fragment slot="footer">
            <div>
              <a href="/services/#identity">About Identity</a>
            </div>
          </Fragment>
        </ServiceCard>
        <ServiceCard class="card-services">
          <Fragment slot="title">Digital Marketing</Fragment>
          <Fragment slot="description">
            <p>
              We help businesses to promote their brand online and create
              engaging digital experiences.
            </p>
          </Fragment>
          <Fragment slot="footer">
            <a href="/services#marketing">About Marketing</a>
          </Fragment>
        </ServiceCard>
        <ServiceCard>
          <Fragment slot="title">Brand Management</Fragment>
          <Fragment slot="description">
            <p>
              We help businesses to implement their brand across all touchpoints
              and ensure consistency.
            </p>
          </Fragment>
          <Fragment slot="footer">
            <a href="/services#management">About Management</a>
          </Fragment>
        </ServiceCard>
      </div>
    </Container>
  </section>
  <section id="work">
    <Container class="spacing-top">
      <h2 class="spacing-intro-title">Our Work</h2>
      <hr class="spacing-divider" />
      <Flex justify="between">
        <Heading element="h3" size="h2" style="margin-bottom: 4rem">
          Selected Work
        </Heading>
        <div>
          <Button>All Work</Button>
        </div>
      </Flex>
      <div class="card-work grid-2-desktop grid-2-tablet">
        {
          work.map((w) => (
            <PhotoCard
              image={w["preview-image"]}
              title={w.title}
              badge={w.category}
            />
          ))
        }
      </div>
    </Container>
  </section>
  <section id="values">
    <Container class="spacing-top">
      <h2 class="spacing-intro-title">Our Values</h2>
      <hr class="spacing-divider" />
      <article class="grid-4-desktop grid-3-tablet grid-2-mobile">
        <ValuesCard variant="blue" style="min-height: 45vh;">
          <Fragment slot="title">Services</Fragment>
          <Fragment slot="description">
            <Heading element="h3" size="h3" style="max-width: 8ch;">
              Our Approach to Branding.
            </Heading>
          </Fragment>
          <Fragment slot="footer">
            <div>
              <Button variant="primary">About Oliv</Button>
            </div>
          </Fragment>
        </ValuesCard>
        <ValuesCard variant="green">
          <Fragment slot="title">Helping Brands</Fragment>
          <Fragment slot="footer">
            <div>
              <Text size="h4" class="no-margin">Bold Moves, Big Impact.</Text>
            </div>
          </Fragment>
        </ValuesCard>
        <ValuesCard variant="muted">
          <Fragment slot="title">How We Work</Fragment>
          <Fragment slot="footer">
            <div>
              <Text size="h4" class="no-margin">
                Honesty is the Best Policy.
              </Text>
            </div>
          </Fragment>
        </ValuesCard>
        <div class="card-values desktop-only"></div>
        <div class="card-values desktop-only"></div>
        <ValuesCard variant="muted" style="min-height: 45vh;">
          <Fragment slot="title">Collaboration</Fragment>
          <Fragment slot="footer">
            <div>
              <Text size="h4" class="no-margin">
                Building Strong Partnerships.
              </Text>
            </div>
          </Fragment>
        </ValuesCard>
        <ValuesCard variant="red">
          <Fragment slot="title">Our Approach</Fragment>
          <Fragment slot="footer">
            <div>
              <Text size="h4" class="no-margin">Achieving Real Results.</Text>
            </div>
          </Fragment>
        </ValuesCard>
        <ValuesCard variant="yellow">
          <Fragment slot="title">Our Philosophy</Fragment>
          <Fragment slot="footer">
            <div>
              <Text size="h4" class="no-margin">
                Building Trust,Not Pitching.
              </Text>
            </div>
          </Fragment>
        </ValuesCard>
      </article>
    </Container>
    <article class="article">
      <Container class="spacing-top">
        <div class="grid-3-2center-desktop grid-2-end-tablet">
          <div class="mobile-hidden"></div>
          <div>
            <Heading element="h3" size="h3">
              Crafting Exceptional Digital Experiences Across All Platforms: Our
              Goal at Øliv.
            </Heading>
            <div class="grid-2">
              <Text size="default">
                At Øliv, we specialize in creating exceptional digital
                experiences that captivate audiences and deliver meaningful
                results. Our goal is to design seamless, user-centered solutions
                across all platforms, blending creativity with cutting-edge
                technology. Whether it’s a stunning website, an engaging app, or
                a cohesive digital strategy, we ensure every touchpoint reflects
                your brand’s essence.
              </Text>
              <Text size="default">
                By understanding your audience and aligning with your business
                goals, we craft experiences that inspire connection, drive
                engagement, and foster growth. At Øliv, we don’t just build
                digital products—we create impactful journeys that leave lasting
                impressions. Let’s transform your vision into extraordinary
                digital realities.
              </Text>
            </div>
          </div>
          <div class="desktop-only"></div>
        </div>
      </Container>
    </article>
  </section>
</main>
<section id="team">
  <Container class="spacing-top">
    <Heading element="h2" size="meta" uppercase="true">Our Team</Heading>
    <hr class="spacing-divider" />
    <Flex justify="between">
      <Heading element="h3" size="h2" style="margin-bottom: 4rem">
        Our Team of Experts
      </Heading>
      <div>
        <Button>About us</Button>
      </div>
    </Flex>
    <div class="grid-4-desktop grid-2-tablet">
      <Card
        variant="green"
        class="card"
        style="padding: 2rem;min-height: 50vh;"
        class="desktop-only"
      >
        <Flex direction="column" align="start" style="height:100%">
          <Heading element="h4" size="meta" uppercase="true">
            Join the team
          </Heading>
          <Heading element="h5" size="h3">
            Want to shape the future of branding?
          </Heading>
          <Text style="margin: 0">
            Join us and create bold, innovative brands that inspire change.
          </Text>
          <Button style="margin-top: auto" variant="primary">Apply Now</Button>
        </Flex>
      </Card>
      <ProfileCard
        title="Annette Black"
        image="./images/annette.webp"
        badge="Branding"
      />
      <ProfileCard
        title="Devon Lane"
        image="./images/devon.webp"
        badge="Design"
      />
      <ProfileCard
        class="desktop-only"
        title="Chris Watson"
        image="./images/chris.webp"
        badge="Marketing"
      />
    </div>
    <div class="desktop-hidden">
      <Card variant="green" class="card" style="margin-top:8rem;padding: 2rem;">
        <Flex direction="column" align="start" style="height:100%">
          <Heading element="h4" size="meta" uppercase="true">
            Join the team
          </Heading>
          <Heading element="h5" size="h3">
            Want to shape the future of branding?
          </Heading>
          <Text style="margin: 0">
            Join us and create bold, innovative brands that inspire change.
          </Text>
          <Button style="margin-top: auto" variant="primary">Apply Now</Button>
        </Flex>
      </Card>
    </div>
  </Container>
</section>
<section id="testimonials">
  <Container class="spacing-top">
    <article class="grid-3-2end-desktop grid-2-end-tablet">
      <div class="desktop-only"></div>
